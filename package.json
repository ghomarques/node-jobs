{
  "name": "node-jobs",
  "version": "1.0.0",
  "description": "Run cron jobs with node.js",
  "scripts": {
    "lint": "eslint .",
    "once": "node . --once --job",
    "job": "node . --job",
    "jobs": "node . ",
    "jobs:watch": "node . --watch",
    "start:pm2": "pm2 stop pm2.json && pm2 delete pm2.json && pm2 start pm2.json && pm2 show node-jobs"
  },
  "repository": {
    "type": "git",
    "url": "git@github.ibm.com:OMTribe/node-jobs.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "command-line-args": "5.2.1",
    "cron": "1.8.2",
    "cron-validator": "1.3.1",
    "dotenv-safe": "~4",
    "log4js": "~4",
    "newrelic": "^7.5.2",
    "require-dir": "1.2.0",
    "type-check": "0.4.0"
  },
  "devDependencies": {
    "eslint": "~4"
  }
}
